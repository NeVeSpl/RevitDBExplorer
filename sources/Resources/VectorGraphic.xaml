    <Style x:Key="IconMemberKind" TargetType="Path" >
        <Setter Property="Fill" Value="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}"/>
        <Setter Property="Stretch" Value="Uniform"/>
        <Setter Property="Width" Value="10"/>
    </Style>

    <Style x:Key="IconProperty" TargetType="Path" BasedOn="{StaticResource IconMemberKind}">
        <Setter Property="Data" Value="{StaticResource vWrench}"/>
        <Setter Property="ToolTip" Value="Property" />
    </Style>
    <Style x:Key="IconMethod" TargetType="Path" BasedOn="{StaticResource IconMemberKind}">
        <Setter Property="Data" Value="{StaticResource vCubeOutline}"/>
        <Setter Property="ToolTip" Value="Method" />
    </Style>
    <Style x:Key="IconStaticMethod" TargetType="Path" BasedOn="{StaticResource IconMemberKind}">
        <Setter Property="Data" Value="{StaticResource vCube}"/>
        <Setter Property="ToolTip" Value="Static Method" />
    </Style>
    <Style x:Key="IconExtra" TargetType="Path" BasedOn="{StaticResource IconMemberKind}">
        <Setter Property="Data" Value="{StaticResource vStar}"/>
        <Setter Property="ToolTip" Value="Extra - not available directly in API" />
    </Style>
    <Style x:Key="IconAsArgument" TargetType="Path" BasedOn="{StaticResource IconMemberKind}">
        <Setter Property="Data" Value="{StaticResource vCubeScan}"/>
        <Setter Property="ToolTip" Value="Parameter of method from different type" />
        <Setter Property="Width" Value="10"/>
    </Style>



    <Style x:Key="IconTree" TargetType="Path" >        
        <Setter Property="Stretch" Value="Uniform"/>
        <Setter Property="Width" Value="10"/>
    </Style>

    <Style x:Key="IconNone" TargetType="Path" BasedOn="{StaticResource IconTree}">
        <Setter Property="Visibility" Value="Collapsed"/>        
    </Style>
    <Style x:Key="IconEmpty" TargetType="Path" BasedOn="{StaticResource IconTree}">
        <Setter Property="Visibility" Value="Hidden"/>
    </Style>
    <Style x:Key="IconProjectParameter" TargetType="Path" BasedOn="{StaticResource IconTree}">
        <Setter Property="Data" Value="{StaticResource vAccountOutline}"/>
        <Setter Property="ToolTip" Value="Project or family parameter" />
    </Style>
    <Style x:Key="IconSharedParameter" TargetType="Path" BasedOn="{StaticResource IconTree}">
        <Setter Property="Data" Value="{StaticResource vAccount}"/>
        <Setter Property="ToolTip" Value="Shared parameter" />
    </Style>


    <Style x:Key="BusyIndicator" TargetType="Path" >
        <Setter Property="Fill" Value="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}"/>
        <Setter Property="Stretch" Value="Uniform"/>
        <Setter Property="Width" Value="20"/>
        <Setter Property="Data" Value="{StaticResource vCogOutline}"/>
    </Style>


    <Style x:Key="IconSelector" TargetType="Path" >
        <Setter Property="Fill" Value="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}"/>
        <Setter Property="Stretch" Value="Uniform"/>
        <Setter Property="SnapsToDevicePixels" Value="true"/>        
    </Style>

    <Style x:Key="IconSelection" TargetType="Path" BasedOn="{StaticResource IconSelector}">
        <Setter Property="Data" Value="{StaticResource vSelectionSearch}"/>       
    </Style>
    <Style x:Key="IconDb" TargetType="Path" BasedOn="{StaticResource IconSelector}">
        <Setter Property="Data" Value="{StaticResource vDatabase}"/>       
    </Style>
    <Style x:Key="IconPickFace" TargetType="Path" BasedOn="{StaticResource IconSelector}">
        <Setter Property="Data" Value="{StaticResource vBorderAllVariant}"/>       
    </Style>
    <Style x:Key="IconPickEdge" TargetType="Path" BasedOn="{StaticResource IconSelector}">
        <Setter Property="Data" Value="{StaticResource vBorderTopVariant}"/>      
    </Style>
    <Style x:Key="IconLinkedElement" TargetType="Path" BasedOn="{StaticResource IconSelector}">
        <Setter Property="Data" Value="{StaticResource vLinkVariant}"/>        
    </Style>
    <Style x:Key="IconDependentElements" TargetType="Path" BasedOn="{StaticResource IconSelector}">
        <Setter Property="Data" Value="{StaticResource vGraph}"/>      
    </Style>
    <Style x:Key="IconActiveView" TargetType="Path" BasedOn="{StaticResource IconSelector}">
        <Setter Property="Data" Value="{StaticResource vPanoramaOutline}"/>      
    </Style>
    <Style x:Key="IconActiveDocument" TargetType="Path" BasedOn="{StaticResource IconSelector}">
        <Setter Property="Data" Value="{StaticResource vFileDocumentOutline}"/>        
    </Style>
    <Style x:Key="IconApplication" TargetType="Path" BasedOn="{StaticResource IconSelector}">
        <Setter Property="Data" Value="{StaticResource vApplication}"/>      
    </Style>
    <Style x:Key="IconSchemas" TargetType="Path" BasedOn="{StaticResource IconSelector}">
        <Setter Property="Data" Value="{StaticResource vFileCabinet}"/>       
    </Style>
    <Style x:Key="IconCategories" TargetType="Path" BasedOn="{StaticResource IconSelector}">
        <Setter Property="Data" Value="{StaticResource vShapeOutline}"/>       
    </Style>
    <Style x:Key="IconForge" TargetType="Path" BasedOn="{StaticResource IconSelector}">
        <Setter Property="Data" Value="{StaticResource vAnvil}"/>
    </Style>
    <Style x:Key="IconReload" TargetType="Path" BasedOn="{StaticResource IconSelector}">
        <Setter Property="Data" Value="{StaticResource vReload}"/>
    </Style>
    <Style x:Key="IconEvent" TargetType="Path" BasedOn="{StaticResource IconSelector}">
        <Setter Property="Data" Value="{StaticResource vFlash}"/>
    </Style>
    <Style x:Key="IconUpdater" TargetType="Path" BasedOn="{StaticResource IconSelector}">
        <Setter Property="Data" Value="{StaticResource vSourceBranchRefresh}"/>
    </Style>
    <Style x:Key="IconLoadedApplications" TargetType="Path" BasedOn="{StaticResource IconSelector}">
        <Setter Property="Data" Value="{StaticResource vPlusBox}"/>
    </Style>
    <Style x:Key="IconAddin" TargetType="Path" BasedOn="{StaticResource IconSelector}">
        <Setter Property="Data" Value="{StaticResource vPlusBox}"/>
    </Style>
